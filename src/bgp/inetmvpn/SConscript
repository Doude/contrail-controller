# -*- mode: python; -*-

Import('BuildEnv')

env = BuildEnv.Clone()
env.Append(CPPPATH = env['TOP'])

libbgp_inetmvpn = env.Library('bgp_inetmvpn',
                           ['inetmvpn_address.cc',
                            'inetmvpn_table.cc',
                            'inetmvpn_route.cc'
                            ])

env.SConscript('test/SConscript', exports='BuildEnv', duplicate = 0)
